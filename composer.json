{
    "name": "kzykhys/ciconia",
    "description": "The Markdown parser for PHP5.4",
    "keywords": ["markdown", "parser", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kazuyuki Hayashi",
            "email": "hayashi@valnur.net"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">5.4.0",
        "symfony/options-resolver": ">=2.8",
        "symfony/console":">=2.8"
    },
    "require-dev": {
        "symfony/finder": ">=2.8",
        "symfony/stopwatch": ">=2.8",
        "symfony/process":">=2.8",
        "kzykhys/pygments": ">=1.0"
    },
    "suggest": {
        "kzykhys/pygments": ">=1.0"
    },
    "bin": ["bin/ciconia"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}